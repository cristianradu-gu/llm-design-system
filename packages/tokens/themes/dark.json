{
  "$schema": "https://tr.designtokens.org/schema.json",
  "theme": {
    "name": { "$type": "string", "$value": "dark" }
  },
  "color": {
    "bg": {
      "canvas": { "$type": "color", "$value": "{color.neutral.900}" },
      "surface": { "$type": "color", "$value": "#111827" }
    },
    "fg": {
      "default": { "$type": "color", "$value": "{color.neutral.0}" },
      "muted": { "$type": "color", "$value": "rgba(255,255,255,0.72)" }
    },
    "border": {
      "default": { "$type": "color", "$value": "rgba(255,255,255,0.14)" }
    },
    "primary": {
      "default": { "$type": "color", "$value": "{color.blue.500}" },
      "hover": { "$type": "color", "$value": "{color.blue.600}" },
      "fg": { "$type": "color", "$value": "{color.neutral.0}" }
    }
  }
}

