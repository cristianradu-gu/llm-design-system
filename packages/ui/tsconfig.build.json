{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "../tsconfig/react-library.json",
  "compilerOptions": {
    "composite": false,
    "noEmit": false,
    "outDir": "dist",
    "rootDir": "src",
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "module": "ESNext",
    "moduleResolution": "Bundler"
  },
  "include": ["src"]
}


